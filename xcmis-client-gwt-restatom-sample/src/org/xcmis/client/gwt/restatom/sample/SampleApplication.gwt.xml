	<inherits name="com.google.gwt.http.HTTP" />
	
	<inherits name='org.xcmis.client.gwt.CmisClientFramework'/>
	
	<inherits name="com.google.gwt.i18n.I18N" />

	<entry-point class='org.xcmis.client.gwt.restatom.sample.client.SampleApplication' />
</module>
